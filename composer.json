{
  "name": "rawr/phpunit-data-provider",
  "type": "library",
  "description": "Lightweight builder for PhpUnit data providers",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Wilkowski",
      "homepage": "https://t-regx.com/"
    }
  ],
  "autoload": {
    "psr-4": {
      "TRegx\\PhpUnit\\DataProviders\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "test/",
      "Test\\Unit\\": "test/Unit/"
    }
  },
  "require": {
    "php": ">=7.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.4 || ^8.0 || ^7.5"
  }
}
