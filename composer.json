{
  "name": "rawr/cross-data-providers",
  "description": "Allows you to mix multiple PhpUnit @dataProvider without duplication",
  "license": "MIT",
  "type": "library",
  "autoload": {
    "psr-4": {
      "TRegx\\DataProvider\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TRegx\\DataProvider\\Test\\": "test/"
    }
  },
  "require": {
    "php": "^7.1 || ^8.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5.20 || ^8.5.20 || 9.5.9",
    "php-coveralls/php-coveralls": "^2.4",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "scripts": {
    "phpcs": "phpcs"
  },
  "scripts-descriptions": {
    "phpcs": "Checks the coding style of the PHP files."
  }
}
